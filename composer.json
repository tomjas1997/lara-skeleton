{
  "name": "invertus/skeleton",
  "type": "prestashop-module",
  "authors": [
    {
      "name": "Invertus",
      "email": "help@invertus.eu"
    }
  ],
  "repositories": [
    {
      "name": "invertus/lara-core",
      "type": "vcs",
      "url": "https://github.com/tomjas1997/laracore.git"
    },
    {
      "name": "invertus/lara-log",
      "type": "vcs",
      "url": "https://github.com/tomjas1997/log.git"
    }
  ],
  "require": {
    "php": "7.1.0",
    "ext-json": "*",
    "apimatic/jsonmapper": "^3.0",
    "apimatic/unirest-php": "^3.0",
    "dusank/knapsack": "^10.0",
    "illuminate/config": "^5.5",
    "illuminate/console": "^5.5",
    "illuminate/container": "^5.5",
    "illuminate/contracts": "^5.5",
    "illuminate/events": "^5.5",
    "illuminate/http": "^5.5",
    "illuminate/support": "^5.5",
    "illuminate/validation": "^5.5",
    "league/container": "2.5.0",
    "league/flysystem": "^1.0",
    "monolog/monolog": "^1.27",
    "prestashop/decimal": "^1.4",
    "rakit/validation": "^1.4",
    "invertus/lara-core": "dev-main",
    "invertus/lara-log": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "Skeleton\\Module\\": "src/"
    },
    "classmap": [
      "src/",
      "skeleton.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Skeleton\\Tests\\": "tests/"
    }
  },
  "config": {
    "platform": {
      "php": "7.1.0"
    },
    "sort-packages": true,
    "preferred-install": "dist",
    "optimize-autoloader": true,
    "prepend-autoloader": false,
    "allow-plugins": {
      "kylekatarnls/update-helper": true
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.19",
    "invertus/prestashop-models": "^1.0",
    "phpunit/phpunit": "^7.5"
  }
}
